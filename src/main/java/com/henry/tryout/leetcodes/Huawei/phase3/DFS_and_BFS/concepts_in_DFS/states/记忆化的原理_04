Map<Integer, Integer> memo = new HashMap<>();

int fib(int n) {
    if (n <= 1) return n;

    // ã€çŠ¶æ€æ£€æŸ¥ã€‘å¦‚æœ çŠ¶æ€n å·²è®¡ç®—è¿‡ï¼Œç›´æ¥è¿”å›
    if (memo.containsKey(n)) {
        return memo.get(n);
    }

    // è®¡ç®— å¹¶ ç¼“å­˜ã€çŠ¶æ€ n çš„ç»“æœã€‘
    int result = fib(n - 1) + fib(n - 2);
    memo.put(n, result); // key = çŠ¶æ€, value = ç­”æ¡ˆ
    return result;
}

